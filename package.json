{
  "name": "gulp-font-npm-files",
  "version": "1.1.2",
  "description": "Get font files from your installed npm packages",
  "main": "index.js",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf coverage reports *.tgz",
    "test": "./node_modules/.bin/nyc --reporter=html mocha --recursive",
    "coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": "https://github.com/yadickson/gulp-font-npm-files",
  "author": "Yadickson Soto <yadickson@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "file-system": "^2.2.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.2.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "should": "^13.1.2",
    "snyk": "^1.78.0"
  },
  "snyk": true
}
